SELECT *,
CASE
WHEN PROFIT_CENTER IS NOT NULL AND PROFIT_CENTER_NAME <> '' 
            THEN LEFT(PROFIT_CENTER_NAME, 3)
        WHEN PROFIT_CENTER IS NOT NULL AND PROFIT_CENTER <> '' 
            THEN PROFIT_CENTER
        ELSE NULL
    END AS PROFIT_CENTER_LOKASI
    FROM PROD_DB.DSTR_BI_COMMUNITY.DEV_F_FINANCE_GL_BANK_CASH_SETTLEMENT dffgbcs 
WHERE dffgbcs.COMPANY_CODE = 'ID90'
AND CLEARING_STATUS = 'OPEN' AND POSTING_KEY = '50';
